{
  "name": "inventory-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "npm run start:api & npm run start",
    "ssr": "npm run start:api & npm run serve:uninversal",
    "ng": "ng",
    "start": "ng serve",
    "start:api": "json-server --watch db.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:uninversal": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:uninversal": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "generate:random-data": "ts-node ./bin/random-data.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/platform-server": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@angular/service-worker": "^5.1.0",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/router-store": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "core-js": "^2.5.2",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "json-server": "^0.12.1",
    "ngx-modialog": "^5.0.0",
    "ramda": "^0.25.0",
    "rxjs": "^5.5.5",
    "tachyons": "^4.9.0",
    "toastr": "^2.1.4",
    "ts-loader": "^3.2.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/language-service": "^5.1.0",
    "@types/jasmine": "~2.8.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.0.57",
    "@types/ramda": "^0.25.8",
    "@types/toastr": "^2.1.35",
    "codelyzer": "~4.0.1",

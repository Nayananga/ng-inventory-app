{
  "name": "inventory-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel start:api serve",
    "start:ssr": "npm-run-all --parallel start:api serve:uninversal",
    "start:api": "json-server --watch db.json",
    "ssr": "npm-run-all start:ssr",
    "ng": "ng",
    "serve": "ng serve",
    "serve:uninversal": "node dist/server.js",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:uninversal": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "generate:random-data": "ts-node ./script/random-data.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.12",
    "@angular/common": "7.2.12",
    "@angular/compiler": "7.2.12",
    "@angular/core": "7.2.12",
    "@angular/forms": "7.2.12",
    "@angular/http": "7.2.12",
    "@angular/platform-browser": "7.2.12",
    "@angular/platform-browser-dynamic": "7.2.12",
    "@angular/platform-server": "7.2.12",
    "@angular/router": "7.2.12",
    "@angular/service-worker": "7.2.12",
    "@ngrx/effects": "6.1.0",
    "@ngrx/router-store": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@ngrx/store-devtools": "6.1.0",
    "@nguniversal/express-engine": "7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "7.1.1",
    "core-js": "2.5.7",
    "express": "4.16.4",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "json-server": "0.14.2",
    "ramda": "0.26.1",
    "rxjs": "6.4.0",
    "tachyons": "4.11.1",
    "toastr": "2.1.4",
    "zone.js": "0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.8",
